/* Modal */
.modal.sarto {
  position: fixed; 
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  display: none;
  justify-content: center;
  align-items: flex-start; /* or center */
  z-index: 1000; /* depending on what z-index values exist in your page, in order for the modal layer to sit on top of other content */
  overflow: auto; /* enable scrolling if needed - although this is not really necessary since we are going take care of overflow in modal-content */
  background-color: rgba(0,0,0,0.8);
}
.modal.sarto.modal-show {
  animation: fadeIn 0.1s ease-in-out forwards;
}
.modal.sarto.modal-hide {
  animation: fadeOut 0.1s ease-in-out 0.1s forwards;
}
.sarto .modal-content {
  position: relative;
  background-color: #fff;
  margin: 2rem;
  padding: 2rem;
  border-radius: 0.25rem;
  width: 50%;
  max-height: 75%;
  overflow: auto;
  box-shadow: 0 4px 20px rgba(0,0,0,0.4);
}
.modal.sarto.modal-show .modal-content {
  animation: fadeInDown 0.3s ease-in-out forwards;
}
.modal.sarto.modal-hide .modal-content {
  animation: fadeOutUp 0.2s ease-in-out forwards;
}
.sarto .modal-content h1 {
    text-align: center;
    margin-bottom: 2rem;
    font-weight: 900;
    text-transform: uppercase;
}
.sarto .modal-content p {
  margin: 1rem 0;
  line-height: 1.5rem;
}
.sarto .modal-close {
  position: absolute;
  top: 0;
  right: 0;
  font-size: 1.75rem;
  font-weight: bold;
  padding: 0 0.75rem;
  /* width: 2rem;
  height: 2rem;
  display: flex;
  justify-content: center;
  align-items: center; */
  color: rgba(0,0,0,0.2);
  cursor: pointer;
  user-select: none;
}
.sarto .modal-close:hover, .sarto .modal-close:focus {
  color: rgba(0,0,0,0.5);
}

.sarto .modal-content img {
    display: block;
    width: 60%;
    height: auto;
    text-align: center;
    margin: 0 auto;
}

.main-product__block.main-product__block-custom_liquid .size-guide {
    position: absolute;
    right: 0;
    top: 0;
    text-align: right;
}

.main-product__block.main-product__block-custom_liquid .size-guide.no_variant{
  top: -45px;
}

